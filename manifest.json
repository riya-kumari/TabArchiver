{
    "manifest_version": 2,
    "name": "Tab Archiver",
    "version": "0.001",
    "browser_action": {
        "default_popup": "CurrentTabs.html",
        "default_icon" : "monkey_icon.png"
    },
    "content_scripts":[
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["content.js"]
   
        }
    ],
    "permissions": [
        "tabs"
    ]
}
